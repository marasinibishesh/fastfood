{
  "expo": {
    "name": "FastFood",
    "slug": "FastFood",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo1.png",
    "scheme": "fastfood",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "./assets/images/logo1.png",
        "backgroundImage": "./assets/images/logo1.png",
        "monochromeImage": "./assets/images/logo1.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "student-447"
        }
      ],
      [
      "@sentry/react-native/expo",
      {
        "url": "https://sentry.io/",
        "project": "react-native",
        "organization": "student-447"
      }
    ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }

    
  }
}

